{
  "name": "meme-generator",
  "version": "1.0.0",
  "description": "meme generator app using mongodb, express, node and javascript",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "start": "npm run build && node ./build/index.js",
    "start:dev": "nodemon ./src/index.js",
    "build": "npm run clean && npm run build-babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aman-codde/meme-generator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aman-codde/meme-generator/issues"
  },
  "homepage": "https://github.com/Aman-codde/meme-generator#readme",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.5.0",
    "mongoose": "^6.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "nodemon": "^2.0.16"
  }
}
