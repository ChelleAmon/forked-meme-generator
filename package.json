{
  "name": "meme-generator",
  "version": "1.0.0",
  "description": "meme generator app using mongodb, express, node and javascript",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack serve",
    "start": "node ./server/server.js",
    "build": "webpack --mode production"

  },
  "nodemonConfig": {
    "restartable":"rs",
    "ignore": [
      ".git",
      "node_modules",
      "dist/",
      "coverage/"
    ], 
    "watch": [
      "server/"
    ],
    "exec": "node --loader server/server.js --emit",
    "env": {
      "NODE_ENV": "development"
    }, 
    "ext": "js, json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aman-codde/meme-generator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aman-codde/meme-generator/issues"
  },
  "homepage": "https://github.com/Aman-codde/meme-generator#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.5.0",
    "mongoose": "^6.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "babel-loader": "^8.2.5",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.16",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1"
  },
  "keywords": []
}
